  .initcontainer span{\n    font-size: 2em;\n  }\n</style>\n\n<script>\n// @ is an alias to /src\n\nexport default {\n  data() {\n    return {\n      page_1: true,\n      page_2: false,\n      page_3: false,\n      rooms:[\n        {num: 1, id:\"309c\"},\n        {num: 2, id:\"310c\"},\n        {num: 3, id:\"311c\"},\n        {num: 4, id:\"312c\"},\n        {num: 5, id:\"313c\"}\n      ],\n      workers:[\n        {num: 1, name:\"前台\"},\n        {num: 2, name:\"经理\"},\n        {num: 3, name:\"管理员\"},\n      ],\n      chose: 0,\n    }\n  },\n  methods: {\n    guest: function(){\n      this.page_1 = false;\n      this.page_2 = true;\n      this.page_3 = false;\n    },\n    host: function(){\n      this.page_1 = false;\n      this.page_2 = false;\n      this.page_3 = true;\n    },\n    to_room: function(num){\n      this.chose=[1,num];\n      this.$router.push(\"room\");\n    },\n    to_work: function(num){\n      this.chose=[2,num];\n      if (num==1){\n        this.$router.push(\"reception\");\n      }\n      else if (num==2){\n        this.$router.push(\"manager\");\n      }\n    },\n  }\n}\n</script>\n"],"sourceRoot":"src/views"}]}